{
  "name": "myprose",
  "version": "3.0.0",
  "description": "myProse is an environment for structuring writing tasks with the help of generative artificial intelligence and other natural language analysis tools.",
  "type": "module",
  "private": true,
  "browser": "./index.html",
  "scripts": {
    "dev": "npx tsx ./index.ts",
    "build": "npx vike build",
    "build:server": "rollup -c",
    "preview": "cross-env NODE_ENV=production npx tsx ./index.ts",
    "start": "npm run dev",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "prettier": "npx prettier -c src/",
    "eslint": "npx eslint@^9 src",
    "depcheck": "npx knip",
    "precommit": "npm run prettier && npm run eslint && npm test",
    "gen:schema": "npx typescript-json-schema ./tsconfig.gen.json WritingTask --out ./src/lib/WritingTask.schema.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CMUEberlyCenter/eberly-docuscope-wa.git",
    "directory": "frontend"
  },
